<template name="singleTpl">
  <view class="music-container" catchtap="onMusicTap" data-music-id="{{musicId}}" data-coverage-Url="{{coverageUrl}}" data-title="{{title}}">
    <image class="music-img" src="{{coverageUrl}}"></image>
    <text class="music-title">{{title}}</text>
    <text class="author">{{author}}</text>
  </view>
</template>