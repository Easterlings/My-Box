@import "../single-music/singleTpl.wxss";
.music-list-container{
  background-color: #fff;
  display:flex;
  flex-direction: column;
}
.inner-container{
  margin:0 auto 20rpx;
}
.music-head{
  padding:30rpx 20rpx 22rpx;
}
.slogan{
  font-size:28rpx;
}
.more{
  float:right;
}
.more-text{
  vertical-align:middle;
  margin-right:10rpx;
  color: #4A6141;
  font-size:28rpx;
}
.more-img{
  width:12rpx;
  height:20rpx;
  vertical-align: middle;
}
.musics-container{
  display:flex;
  flex-direction:row;
}